$(document).ready(function () {
  $('#c18694 a, #c18695 a').click(function(event) {
    var h;
    h = $(this).closest('.text-teaser').find('.headline').text().trim();
    if (!h) {
      h = $(this).closest('.str_textimageteaser').find('.headline').text().trim();
    }
    if (h) {
      trackEventDatalayer("Teaser", h + ", " + window.location.pathname, $(this).attr('href'));
    }
  });
});
</script>
