	$(function() {
	    if ($(window).width() < 768) {
			$('.str-slick-video .slick-slider').remove();
	    } else {
			$(".videostage video").attr("autoplay","");
		}
		$slickVideoSlider = $('.str-slick-video .slick-slider');
		$slickVideoSliderInitial =  Math.floor(Math.random() * (2) );
			console.log("Playin' video #" + ($slickVideoSliderInitial + 1) );
	    function slickVideoSliderNext(){
		    $slickVideoSlider.slick('slickNext');
	    }
	    function slickVideoSliderPlay(off){
	    	$v = $slickVideoSlider.find('.slick-active video').get(0);
	    	if (!$v) {return;};
	    	if (off) {$v.pause();} else {
	    		$v.play();
	    		$slickVideoSlider.find('video').off('ended');
	    		$slickVideoSlider.find('.slick-active video').on('ended',slickVideoSliderNext);
	    		// $v.on('ended',slickVideoSliderNext);
	    	}
	    }
	    $slickVideoSlider.slick({
	        arrows: false,dots: false,fade: true,autoplay: false,speed: 1,draggable: false, initialSlide: $slickVideoSliderInitial
	    });
	    // $slickVideoSlider.find('video').on('ended',slickVideoSliderNext);
	    // $slickVideoSlider.on('beforeChange', function(event, slick, currentSlide){
	    //   	slickVideoSliderPlay(false);
	    // });
	    $slickVideoSlider.on('afterChange', function(event, slick, currentSlide){
	      	slickVideoSliderPlay();
	    });
		// if ($(window).width() < 768) {
		// if ($(window).width() >= 768) {
			slickVideoSliderPlay();
	    // };
	});
