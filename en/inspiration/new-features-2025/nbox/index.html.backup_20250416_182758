		    $slickVideoSlider.slick('slickNext');
	    }
	    function slickVideoSliderPlay(off){
	    	$v = $slickVideoSlider.find('.slick-active video').get(0);
	    	if (!$v) {return;};
	    	if (off) {$v.pause();} else {
	    		$v.play();
	    		$slickVideoSlider.find('video').off('ended');
	    		$slickVideoSlider.find('.slick-active video').on('ended',slickVideoSliderNext);
