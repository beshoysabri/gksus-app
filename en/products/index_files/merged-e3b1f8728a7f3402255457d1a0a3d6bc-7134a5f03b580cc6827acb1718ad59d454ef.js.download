$(function() {

  $filterLogic = function () {
    var filterAll = {};
    var filterAllPlain = $('#filterForm option:not(:selected)').map(function() { 
        if ($(this).val()) {
          return $(this).parent().attr('name') + '-' +$(this).val();
        }
      }).get();
      for (var i = 0, len = filterAllPlain.length; i < len; i++) {
        filterAll[filterAllPlain[i]] = 1;
      }
    var classes = {};
    var foundItems = 0;
    // $('.card.a-fadeInUp').each(function(index, el) {
    $('.card.found').each(function(index, el) {
        foundItems++;
        // console.log(foundItems);
        var c = $(this).attr('class').toString().split(' ');
        for (var i = 0, len = c.length; i < len; i++) {
          if (!filterAll[c[i]]) {continue;};
          classes[c[i]] = 1;
        }
    });
    $('.filter-count').text('('+foundItems+')');
        // console.log(classes);
      $('#filterForm select option').prop('disabled', false);

      $('#filterForm select, .filter-holder').removeClass('sel');
      $('#filterForm select').each(function(index, el) {
          if( sel = $(this).prop('selectedIndex')){
            // console.log($(this).attr('name') +': '+sel);
            $(this).addClass('sel');
            $('.filter-holder').addClass('sel');
          }
      });

      for (var i = 0, len = filterAllPlain.length; i < len; i++) {
        if (classes[filterAllPlain[i]] == 1) {continue};
        var sp = filterAllPlain[i].split('-');
        $('#filterForm select[name="'+sp[0]+'"] option[value="'+sp[1]+'"]').prop('disabled', true);
      }
    $('#filterForm select').selectric('refresh');
    // $(window).trigger("lookup.unveil");

  };
});

