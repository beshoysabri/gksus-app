$(function (){
  $('.js-tab-accordion').on('click', function(e) {
    e.preventDefault();
    var $this = $(this);
    var $collapse = $($this.data('target'));
    $collapse.collapse('toggle').toggleClass('active');
    var $tabs = $this.closest('.tabs');
    $tabs.find('.nav-tabs li').removeClass('active');
    $tabs.find('.nav-tabs [href="' + $this.data('target') + '"]').parent().addClass('active');
});

  $(document).on('shown.bs.tab', '.tabs .nav-tabs a[data-toggle="tab"]', function (e) {
    $(this).closest('.nav-tabs').find('li').removeClass('active');
    $(this).closest('li').addClass('active');
    $($(e.target).attr('href')).css('height','');
    $($(e.target).attr('href')).addClass('collapse in');

    $(this).closest('.tabs').find('.accordion-header').removeClass('collapsed');
    $($(e.target).attr('href') + '-acc .accordion-header').addClass('collapsed');
    

    $(window).trigger('resize');
    if ($('.str-slick-vertical .slick-slider').length > 0) {
      $('.str-slick-vertical .slick-slider').slick('reinit');
    };

  });

  $('.tabs').each(function() {
    // console.log('tabs each');
    $(this).find('.nav-tabs > li:first-child a').tab('show');
  });  

  if (window.location.hash != "") {
    var th = window.location.hash.substr(1);
    if (th.indexOf("&") < 0 && $('#tab-click-' + th).length > 0) {
      $('#tab-click-' + th).click();
      $(window).scrollTop(0,0);
      $('html,body').animate({
        scrollTop: $('#tab-click-' + th).offset().top - 100
      }, 500, function() {});
    }
  };

});





