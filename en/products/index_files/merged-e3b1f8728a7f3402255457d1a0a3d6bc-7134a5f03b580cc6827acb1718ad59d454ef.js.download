$(function() {
  // $('.card-listing .card').addClass('hidden-o animated a-fadeInUp');

  // $filterLogic();
  $slideCards = function ($cards){
    $cards.addClass('hidden-o animated a-fadeInUp').show();
    $(window).trigger("lookup.unveil");
    // $cards.each(function(index, el) {
    //   // $(this).addClass('hidden-o animated a-fadeInUp').show();
    //   // $(this).addClass('a-fadeInUp');

    //   $(this).delay(index * 100).queue(
    //     function() {
    //       $(this).addClass('hidden-o animated a-fadeInUp').show();
    //       $(this).find('img').trigger( "unveil" )
    //       // $(window).trigger("lookup.unveil");
    //       $(this).dequeue();
    //     }
    //   );        
    // });
  }

  // $slideCards($('.card-listing .card'));

  $('.filter-reset').click(function(event) {
    event.preventDefault();
    console.log('reset');
    $('#filterForm select').prop('selectedIndex',0);
    $('#filterForm').change();
  });

  $('#filterForm').change(function(event) {
    $('.card-listing .card').removeClass('a-fadeInUp');
    $('.card-listing .card').removeClass('found').hide();
    var f = $('#filterForm').serialize();
    var parts = f.split('&');
    var p = "";
    var pCollect = "";
    var count = 0;
    // f = f.replace('=','-');

    for (var i = 0, len = parts.length; i < len; i++) {
      pp = parts[i].split('=');
      if (!pp[1]) {continue;};
      if (pp[1] == 0) {continue;};
      count++;
      p = "." + pp[0] + '-' + pp[1];
      // alle, oder
      // $('.card-listing .card'+p).addClass('a-fadeInUp').show();
      pCollect += p;
    }

      // console.log(count);
      // console.log(pCollect);

      if (count == 0) {
        // $('.card-listing .card').addClass('a-fadeInUp').show();
        $('.card-listing .card').addClass('found');
        $slideCards($('.card-listing .card'));
      } else {
        // alle, und
        // $('.card-listing .card' + pCollect).addClass('a-fadeInUp').show();
        $('.card-listing .card' + pCollect).addClass('found');
        $slideCards($('.card-listing .card.found' + pCollect));

      };
      $(window).trigger("resize");

      $filterLogic();
  });

  $(".kfilter.fronts select[name='kitchenColor'] option[value='5']").remove();

  $('#filterForm select').selectric();
  $('#filterForm').change();
});


