$(function() {


  // $('.filter-reset').click(function(event) {
  //   event.preventDefault();
  //   $('#filterFormJobs select').prop('selectedIndex',0);
  //   $('#filterFormJobs').change();
  // });

  $('#filterFormJobs').change(function(event) {
    $('.jobs-list-entry .row').hide();
    // $('select[name="remit"]').prop('disabled', true);

      if ($('select[name="typ"]').val() == 'Direkteinstieg') {
          $('select[name="remit"]').prop('disabled', false);
      } else {
        $('select[name="remit"] option[value="all"]').prop('selected', true);
        $('select[name="remit"]').prop('disabled', true);

      }

    var f = $('#filterFormJobs').serialize();
    var parts = f.split('&');
    var p = "";
    var pCollect = "";
    var count = 0;

    for (var i = 0, len = parts.length; i < len; i++) {
      pp = parts[i].split('=');
      // console.log(pp);
      if (!pp[1]) {continue;};
      if (pp[1] == 'all') {continue;};
      if (pp[1] == 0) {continue;};
      p = '[data-filter-' + pp[0] + '="' + decodeURIComponent(pp[1].replace(/\+/g, '%20')) + '"]';
      count++;
      pCollect += p;
    }

      if (count == 0) {
        $('.jobs-list-entry .row').show();
      } else {
        $('.jobs-list-entry .row' + pCollect).fadeIn();

      };
      $('#filterFormJobs select').selectric('refresh');
      // $(window).trigger("resize");
  }).change();

  if (window.location.hash != "") {
    $('select[name="typ"] option[value="'+window.location.hash.substr(1)+'"]').prop('selected', true);
    $('#filterFormJobs').change();
    
  };

  // $('#filterFormJobs select').selectric();
  // $('#filterFormJobs').change();
});

