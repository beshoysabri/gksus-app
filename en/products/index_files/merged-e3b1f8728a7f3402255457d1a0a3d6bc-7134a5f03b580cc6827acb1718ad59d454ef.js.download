  $( function(){


    var coo = Cookies.getJSON();
    var cooStore = coo['basket'];
    if (cooStore == undefined) {cooStore={}};

    for (var key in cooStore) {
       if (cooStore.hasOwnProperty(key)) {
          var obj = cooStore[key];
          $('.basket-add-to[data-id="'+key+'"]').addClass('pinned');
          $('.nav-icons').addClass('pinned');
          $('.merkzettel-meta').addClass('pinned');
          $('#menu-bottom').addClass('pinned');
          $('.basket-add-to[data-id="'+key+'"] .label').text(lll['basketremove']);
          $('.basket-add-to[data-id="'+key+'"]').attr('title',lll['basketremove']);
        }
    }

    $('.basket-add-to').click(function(event) {
        event.preventDefault();
        var toBePinned = $(this).data('id');

        if ($(this).hasClass('pinned')) {
            delete cooStore[toBePinned];
            if ($('body').hasClass('p-66')) {
                $('.basket-add-to[data-id="'+toBePinned+'"]').each(function(index, el) {
                  $(this).closest('.card').remove();
                  $(this).closest('.content-block').remove();
                });
                $('.card-listing').each(function(index, el) {
                  // if ($(this).find('.card').length == 0) {$(this).remove();};
                  if ($(this).find('.card').length == 0 && $(this).find('.content-block').length == 0) {$(this).remove();};
                });
                if ($('.card-listing').length == 0) {
                  $('.teaser-emptymessage').removeClass('hidden').addClass('hidden-o a-fadeInUp animated');
                  $('.teaser-download').remove();
                };

                

            };
            $(this).removeClass('pinned');
            $(this).attr('title',lll['basketadd']);
            $(this).find('.label').text(lll['basketadd']);
        } else {
            cooStore[toBePinned] = 1;
            $(this).attr('title',lll['basketremove']);
            $(this).find('.label').text(lll['basketremove']);
            $(this).addClass('pinned');
        }
        $('.nav-icons').removeClass('pinned');
        $('.merkzettel-meta').removeClass('pinned');
        if (Object.keys(cooStore).length > 0) {
          setTimeout(function(){ 
            $('.nav-icons').addClass('pinned'); 
            $('.merkzettel-meta').addClass('pinned'); 
          }, 300);
          
        };
        Cookies.set('basket', cooStore);

    });

  });


